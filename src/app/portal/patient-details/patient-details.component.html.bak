<!-- Pregnancy info -->
<div class="modal  pregnancy_info" id="pregnancy_info" tab-index="-1">
    <div class="modal-dialog modal-dialog-centered" style="max-width:60%">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="m-0">Female Pregnancy Information</h5>
                <div class="d-flex align-items-center gap-2">
                    <div class="d-flex align-items-center gap-2">
                        <button class="btn btn-main-fill">Save</button>
                        <button class="btn btn-danger-outline">Close</button>
                    </div>
                    <button type="button" data-bs-dismiss="modal" aria-label="Close" class="btn-close"></button>
                </div>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-md-3">
                        <div class="d-flex flex-column">
                            <label for="" class="form-label">Pregnancy</label>
                            <div class="clinical_toggle toggle__switch switch-sm d-inline-flex align-self-start">
                                <button class="fs-7 btn selected">No</button>
                                <button class="fs-7 btn">Yes</button>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-3">
                        <div class="d-flex flex-column">
                            <label for="" class="form-label">Lactation</label>
                            <div class="clinical_toggle toggle__switch switch-sm d-inline-flex align-self-start">
                                <button class="fs-7 btn selected">No</button>
                                <button class="fs-7 btn">Yes</button>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-3">
                        <div class="d-flex flex-column">
                            <label for="" class="form-label">Tri Semester</label>
                            <select name="" class="form-select" id="">
                                <option value="">1st Tri Semester</option>
                                <option value="">2nd Tri Semester</option>
                                <option value="">3rd Tri Semester</option>
                            </select>
                        </div>
                    </div>
                    <div class="col-md-3">
                        <div class="d-flex flex-column">
                            <label for="" class="form-label">Contraception</label>
                            <select name="" class="form-select" id="">
                                <option value="">Not Selected</option>
                                <option value="">PILLS</option>
                                <option value="">IUD</option>
                                <option value="">NONE</option>
                            </select>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


