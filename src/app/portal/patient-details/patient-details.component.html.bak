                                <div class="viewPatientsAllergiesModal" data-bs-backdrop="static" tabindex="-1" id="viewAllergies" tabindex="-1" >
                                    <div class="modal-dialog modal-dialog-scrollable modal-l ">
                                        <div class="modal-content p-3">
                                            <div class="modal-header">
                                                <h6 class="modal-title" translate>Allergies</h6>                                            
                                            </div>
                                            <div class="modal-body">
                                                <div class="d-flex w-100 align-items-center justify-content-between">
                                                    <div class="d-flex w-100 mt-2">
                                                        <div [innerHtml]="pl.AllergyData"></div>
