 
    <div class="row g-0">
        <div class="main-content col">
            <!--Container-->
            <div class="container-fluid mt-4" >
                <div class="container__wrapper handoverPage">
                    <div class="page-header sticky-top justify-content-between">
                        <h2 class="page-heading">{{langData?.common?.PatientSummary}}</h2>
                        <form [formGroup]="visitsForm">
                            <div class="d-flex align-items-center">
                            <label for="" class="form-label mb-0 px-2">{{langData?.common?.Visits}}</label>
                            
                            <select class="form-select pe-4" style="width:450px;" formControlName="visitId"
                                (change)="onVisitsChange($event)">
                                <option dir="ltr" value="" selected translate>{{langData?.common?.SelectVisit}}</option>
                                <option dir="ltr" *ngFor="let visit of PatientVisitsList" [value]="visit.AdmissionID">
                                    {{visit.VisitDate}} - {{visit.DoctorName}}
                                </option>
                            </select>

                            <!-- <button type="button" class="btn btn-main-fill ms-2" (click)="BackToPatients()" translate>Back</button> -->
                       </div>
                       <!-- <div class="actions ms-auto pe-2">
                        
                    </div> -->
                    </form>
