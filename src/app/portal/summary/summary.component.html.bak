                                                    <path d="M23.9998 25.6975C17.7821 25.6975 12.7459 20.0572 12.7459 13.0681C12.7459 6.14031 17.7821 0.5 23.9998 0.5C30.2175 0.5 35.2536 6.20162 35.2536 13.1294C35.2536 20.0572 30.2175 25.6975 23.9998 25.6975Z" fill="#41A5B4"/>
                                                    <path d="M34.414 25.0267C34.7549 24.951 35.1105 24.9711 35.4401 25.0844H35.4405C43.6478 27.9046 48 34.3422 48 43.6607C48 44.1486 47.8035 44.6165 47.4535 44.9612C47.104 45.3063 46.6295 45.5 46.1346 45.5H1.86538C1.37051 45.5 0.896033 45.3063 0.546471 44.9612C0.196465 44.6165 0 44.1486 0 43.6607C0 34.3421 4.35209 27.9048 12.5595 25.0844C12.8892 24.9711 13.2448 24.951 13.5857 25.0267C13.9261 25.1028 14.2384 25.2713 14.4869 25.5138C16.6008 27.5981 20.145 28.8242 23.9998 28.8242C27.8546 28.8242 31.3989 27.5981 33.5127 25.5138C33.7613 25.2713 34.0735 25.1028 34.414 25.0267Z" fill="#41A5B4"/>
                                                </svg>                                                                             
                                            </div>
                                            <p class="fw-bold mb-2 text-center">{{patientName}}</p>
                                            <p class="text-center" dir="ltr">{{age}} / {{gender}}</p>
                                        </div>
                                    </div>
                                    <div class="col-xl-10 col-md-9">
