                        <div class="col-xl-6 col-lg-12 col-md-12">
                            <div class=" card-3d-2 position-relative h-100">
                                <div class="row gx-2">
									<div class="col-md-9" style="height:260px" [ngStyle]="{'display':displayLab}">
                                        <div class="card-3d-wrapper h-100 p-2">
                                        <div class="row-items"
                                            *ngFor="let inv of PatientInvestigationsList;let i=index">
                                            <p class="col-2">{{inv.ItemCode}}</p>
                                            <p class="col-8">{{inv.ItemName}}</p>
                                            <p class="col-3" dir="ltr">{{inv.CreateDate}}</p>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-9" style="height:260px" [ngStyle]="{'display':displayRad}">
                                    <div class="card-3d-wrapper h-100 p-2">
                                        <div class="row-items"
                                            *ngFor="let inv of PatientRadiologyList;let i=index">
                                            <p class="col-2">{{inv.ItemCode}}</p>
                                            <p class="col-8">{{inv.ItemName}}</p>
                                            <p class="col-3" dir="ltr">{{inv.CreateDate}}</p>
                                        </div>
                                    </div>
                                </div>
                                    <div class="col-md-3">
                                        <div class="card-3d-heading">
