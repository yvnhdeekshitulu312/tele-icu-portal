                           <div class="toggle__location d-inline-block ms-3 mt-2 float-end">
                            <div class="toggle__swithcer">
                                <button [class]="btnSuwaidi" (click)="changeLocation('2')">{{langData?.common?.Suwaidi}}</button>
                                <button [class]="btnNuzha" (click)="changeLocation('3')">{{langData?.common?.Nuzha}}</button>
                            </div>
