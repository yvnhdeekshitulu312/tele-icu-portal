<div class="main-content col">
    <div class="content-wrapper">
        <div class="row ">
            <div class="col-lg-12">
                <div class="sub-heading justify-content-between align-items-center border-bottom">
                    <h4> Appointments
                    </h4>
                    <div class="actions d-flex align-items-center flex-wrap">
                        <form [formGroup]="tableAppointmentsForm" (keydown.enter)="$event.preventDefault()">
                            <div
                                class="d-flex w-100 align-items-center justify-content-between">
                                <div class="d-flex g-2 flex-md-nowrap flex-wrap">
                                    <div class="d-flex align-items-center mb-1">
                                        <label for=""
                                            class="form-label mb-0 px-2 text-nowrap"
                                            translate>From Date</label>
                                        <mat-form-field appearance="fill">
                                            <mat-label translate>dd-MMM-yyyy
                                            </mat-label>
                                            <input matInput class=""
                                                formControlName="wmFromDate"
                                                [matDatepicker]="wmFromDate"
                                                (click)="wmFromDate.open()">
                                            <mat-datepicker-toggle matSuffix
                                                [for]="wmFromDate">
                                            </mat-datepicker-toggle>
                                            <mat-datepicker #wmFromDate>
                                            </mat-datepicker>
                                        </mat-form-field>
                                    </div>
            
                                    <div class="d-flex align-items-center mb-1">
                                        <label for=""
                                            class="form-label mb-0 px-2 text-nowrap"
                                            translate>To Date</label>
                                        <mat-form-field appearance="fill">
                                            <mat-label translate>dd-MMM-yyyy
                                            </mat-label>
                                            <input matInput class=""
                                                formControlName="wmToDate"
                                                [matDatepicker]="wmToDate"
                                                (click)="wmToDate.open()">
                                            <mat-datepicker-toggle matSuffix
                                                [for]="wmToDate">
                                            </mat-datepicker-toggle>
                                            <mat-datepicker #wmToDate>
                                            </mat-datepicker>
                                        </mat-form-field>
                                    </div>
                                </div>
                                <div class="d-flex">
                                    <div class="">
                                        <button type="button"
                                            class="btn btn-main-fill mx-2" (click)="fetchDoctorAppointments()"
                                            translate >Search</button>
                                    </div>
                                    <!-- <div class="">
                                    <button type="button" class="btn btn-main-outline" translate>Clear</button>
                                </div> -->
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
        
        <div class="p-3" *ngFor="let al of appointmentList;let i=index">
            <!-- <h2 class="clr-main fw-bold mb-2">{{al.ScheduleDate}}</h2> -->

            <div class="card border rounded-2 p-3 mt-1">
                <div class="d-flex align-items-center justify-content-start gap-2 mb-3">
                    <span class="d-flex align-items-center justify-content-center">
                        <i  *ngIf = "al.IsVideoConsultation == true" class="fa fa-video-camera menu-icon" style="color: #69BAC9;  font-size: 1.5rem;"></i>
                        <i *ngIf = "al.IsVideoConsultation == false" class="fa fa-clinic-medical menu-icon" style="color: #69BAC9;    font-size: 1.5rem;"></i>
                    </span>
                    
                    <h2 class="mb-0 clr-main">{{al.ScheduleDate}} {{tConvert(al.ScheduleStartTime)}}</h2>
                </div>
                <div class="row gx-3">
                    <div class="col-4">
                        <div class="p-3 d-flex align-items-start">
                           <span class="border rounded-pill me-2 p-1">
                            <svg width="40" height="40" viewBox="0 0 51 51" fill="none" xmlns="http://www.w3.org/2000/svg">
                                <path d="M25.5364 27.8142C20.8732 27.8142 17.0961 23.584 17.0961 18.3421C17.0961 13.1463 20.8732 8.91608 25.5364 8.91608C30.1997 8.91608 33.9768 13.1923 33.9768 18.3881C33.9768 23.584 30.1997 27.8142 25.5364 27.8142Z" fill="#41A5B4"/>
