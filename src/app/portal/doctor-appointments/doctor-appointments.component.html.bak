                        </div>
                    </div>
                </div>
                <div class="d-flex align-items-center justify-content-end mt-4 gap-2">
                    <!-- <button class="btn btn-main-outline me-2">Cancel</button>
                    <button class="btn btn-main-fill me-2">Reschedule</button> -->
                    <button class="btn btn-sm btn-danger-outline mx-1">
                                    Cancel
                                </button>
                                <button class="btn btn-sm btn-main-outline mx-1">
                                   Reschedule
                                </button>
                    <button class="btn btn-main-fill" *ngIf = "al.IsVideoConsultation == true" (click)="startConsultation(al)">Start Consultation</button>
                </div>
            </div>
        </div>
        
    </div>
</div>
